// Create an array called favSingers and store your three favorite singers in it.
const favSingers = ["Taylor Swift", "Beyonce", "Adele"];

// Log the first singer in the array using array index notation.
console.log(favSingers[0]);

// Create an array called favNumbers and store four of your favorite numbers.
const favNumbers = [1, 7, 13, 21];

// Create an array named mixedArr and store different data types: ["string", ["otherarray"], 123, true].
const mixedArr = ["string", ["otherarray"], 123, true];

// Access and log each item in the mixedArr using array index notation.
console.log(mixedArr[0]);
console.log(mixedArr[1]);
console.log(mixedArr[2]);
console.log(mixedArr[3]);
